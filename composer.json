{
    "name": "mikp/wn-openidconnect-plugin",
    "description": "OpenID Connect extension to Social Login plugin",
    "homepage": "https://github.com/mik-p/wn-openidconnect-plugin",
    "type": "winter-plugin",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "mik-p",
            "email": "mppritchard3@hotmail.com"
        }
    ],
    "repositories": [
        {
